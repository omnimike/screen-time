{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "js-build": "webpack",
    "js-watch": "webpack --watch",
    "css-build": "node-sass -o server/static client/main.scss",
    "css-watch": "node-sass -o server/static -w client/main.scss ",
    "build": "npm-run-all --parallel js-build css-build",
    "watch": "npm-run-all --parallel js-watch css-watch"
  },
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "bootstrap": "^4.0.0",
    "eslint": "^4.17.0",
    "eslint-plugin-flowtype": "^2.44.0",
    "eslint-plugin-react": "^7.6.1",
    "node-sass": "^4.7.2",
    "npm-run-all": "^4.1.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "webpack": "^3.11.0",
    "webpack-merge": "^4.1.2"
  }
}
